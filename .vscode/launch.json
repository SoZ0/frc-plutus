            "name": "Launch Main",
            "request": "launch",
            "mainClass": "ca.sozoservers.dev.Main",
            "vmArgs": "--module-path C:/javafx-sdk-17.0.1/lib --add-modules javafx.controls,javafx.base,javafx.fxml,javafx.graphics,javafx.media,javafx.swing,javafx.web,javafx.fxml",
            "projectName": "plutusfx"
